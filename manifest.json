{
  "manifest_version": 2,

  "name": "Special Symbols input",
  "description": "This extension show a little keyboard with special characters.",
  "version": "0.1",
  "icons": { "16": "icon16.png",
             "48": "icon48.png",
             "128": "icon128.png"} ,

  "browser_action": {
    "default_icon": {
      "19": "icon19.png",
      "38": "icon38.png"
    },
    "default_popup": "popup.html"
  },

  "permissions": [
    "tabs", "http://*/*", "https://*/*"
  ]
}
